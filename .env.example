# Docker 镜像
DOCKER_IMAGE=ghcr.io/doudou-start/chatgpt-register:latest

# 服务端口
PORT=8082

# 数据目录（Docker 环境下无需修改）
DATA_DIR=/app/data

# 日志目录（Docker 环境下无需修改）
LOG_DIR=/app/logs

# 调试模式
DEBUG=false

# API Key（可选，不配置则不启用鉴权）
# 强烈建议在生产环境设置，用于保护 API 访问
API_KEY=

# 电话验证码 API 配置
# SMS-Activate 接码平台 API Key
PHONE_API_KEY_SMS_ACTIVATE=

# 5SIM 接码平台 API Key
PHONE_API_KEY_5SIM=

# 授权服务器地址（可选，用于 License 激活和版本更新检查）
LICENSE_SERVER_URL=
